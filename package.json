        "eslint-config-cheminfo": "^1.20.1",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jest": "^22.4.1",
        "jest": "^24.5.0",
        "rimraf": "^2.6.3"
      },
      "dependencies": {
          "openchemlib-extended": "6.0.0"
      }

