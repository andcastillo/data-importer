        "compile": "babel src --out-dir lib --quiet --ignore __tests__",
        "prepublish": "npm run compile"
      },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "cheminfo-tools": "^1.23.3",
